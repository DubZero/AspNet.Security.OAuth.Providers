{
  "version": "1.0.0-beta2-*",

  "description": "ASP.NET Core security middleware enabling Yahoo authentication.",
  "authors": [ "Kévin Chalet", "Jerrie Pelser" ],

  "packOptions": {
    "owners": [ "Kévin Chalet", "Jerrie Pelser" ],

    "projectUrl": "https://github.com/aspnet-contrib/AspNet.Security.OAuth.Providers",
    "iconUrl": "https://avatars3.githubusercontent.com/u/7998081?s=64",
    "licenseUrl": "http://www.apache.org/licenses/LICENSE-2.0.html",

    "repository": {
      "type": "git",
      "url": "git://github.com/aspnet-contrib/AspNet.Security.OAuth.Providers"
    },

    "tags": [
      "aspnetcore",
      "authentication",
      "oauth",
      "security",
      "yahoo"
    ]
  },

  "buildOptions": {
    "warningsAsErrors": true,
    "nowarn": [ "CS1591" ],
    "xmlDoc": true
  },

  "dependencies": {
    "NETStandard.Library": "1.6.0",
    "AspNetCore.Security.OAuth.Extensions": {
      "target": "project",
      "type": "build"
    },
    "JetBrains.Annotations": {
      "type": "build",
      "version": "10.1.4"
    },
    "Microsoft.AspNetCore.Authentication.OAuth": "1.0.0"
  },

  "frameworks": {
    "netstandard1.6": {
      "imports": "dnxcore50"
    }
  }
}
